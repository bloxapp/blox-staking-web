{
  "name": "blox-staking-web",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7"
  },
  "scripts": {
    "postinstall": "cd client && yarn",
    "build": "cd client && yarn build",
    "client": "cd client && yarn start",
